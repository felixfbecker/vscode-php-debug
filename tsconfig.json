{
  "exclude": [
    "node_modules",
    "out"
  ],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "out",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true
  }
}
